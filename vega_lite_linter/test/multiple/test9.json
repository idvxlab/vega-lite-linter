{
    "data": {
        "url": "data/seattle-weather.csv"
    },
    "mark": "point",
    "encoding": {
        "x": {
            "field": "date",
            "type": "temporal",
            "aggregate": "mean"
        },
        "y": {
            "field": "temp_min",
            "type": "quantitative",
            "scale": {
                "type": "log"
            }
        },
        "size": {
            "field": "weather",
            "type": "nominal"
        }
    }
}