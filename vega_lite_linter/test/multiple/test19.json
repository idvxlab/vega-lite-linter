{
    "data": {
        "url": "data/seattle-weather.csv"
    },
    "mark": "point",
    "encoding": {
        "x": {
            "field": "weather",
            "type": "nominal",
            "scale": {
                "zero": true
            }
        },
        "y": {
            "field": "wind",
            "type": "quantitative"
        },
        "size": {
            "field": "temp_min",
            "type": "quantitative"
        }
    }
}