{
    "data": {
        "url": "data/seattle-weather.csv"
    },
    "mark": "line",
    "encoding": {
        "x": {
            "field": "temp_min",
            "type": "quantitative",
            "bin": {
                "maxbins": 10
            }
        },
        "y": {
            "field": "temp_max",
            "type": "quantitative",
            "aggregate": "min",
            "bin": {
                "maxbins": 10
            }
        }
    }
}