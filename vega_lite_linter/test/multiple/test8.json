{
    "data": {
        "url": "data/cars.json"
    },
    "mark": "bar",
    "encoding": {
        "x": {
            "field": "Origin",
            "type": "nominal",
            "aggregate": "min"
        },
        "y": {
            "field": "Miles_per_Gallon",
            "type": "quantitative",
            "stack": "normalize",
            "scale": {
                "zero": false
            }
        },
        "color": {
            "field": "Cylinders",
            "type": "quantitative"
        }
    }
}