{
    "data": {
        "url": "data/stocks.csv"
    },
    "transform": [
        {
            "filter": "datum.symbol==='GOOG'"
        }
    ],
    "mark": "line",
    "encoding": {
        "x": {
            "field": "date",
            "type": "temporal",
            "aggregate": "count",
            "scale": {
                "zero": false
            }
        },
        "y": {
            "type": "quantitative",
            "aggregate": "count"
        }
    }
}