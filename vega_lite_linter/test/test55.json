{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {"url": "data/barley.json"},
    "mark": "bar",
    "encoding": {
        "x": {
            "field": "variety",
            "type": "nominal"
        },
        "y": {
            "aggregate": "sum",
            "field": "yield",
            "type": "quantitative",
            "scale": {
                "zero": false
            }
        }
    }
  }
  